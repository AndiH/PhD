	Eprint = {0903.3905},
	Keywords = {panda, physics},
	Month = mar,
	Primaryclass = {hep-ex},
	Title = {{Physics Performance Report for PANDA: Strong Interaction Studies with Antiprotons}},
	Url = {http://arxiv.org/abs/0903.3905},
	Year = 2009,
